<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.klauncher.setupwizard">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_MULTICAST_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application android:allowBackup="true"
        android:supportsRtl="true">

        <activity android:name=".vivo.VivoSetupActivity"
            android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen"
            >
        </activity>

        <activity android:name=".hw.HwLanguageSettings"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            ></activity>

        <activity android:name=".hw.HwWifiSettings"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            ></activity>
        <activity android:name=".hw.HwAgreement"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            ></activity>
        <activity android:name=".hw.HwSetupComplete"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            ></activity>

        <service android:name=".SetupService"
            android:enabled="true"
            android:exported="true"
            ></service>
        <activity android:name=".oppo.OppoLanguageSettings"
                  android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            ></activity>
        <activity android:name=".oppo.OpWifiSettings"
                  android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            ></activity>
        <activity android:name=".oppo.OpAgreement"
                  android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            ></activity>
        <activity android:name=".oppo.OpSetupComplete"
                  android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            ></activity>
    </application>

</manifest>
